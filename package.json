{
  "name": "runtime",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "tsc",
    "sass": "sass -w fruitUI/src/scss/lib:css",
    "prod": "sass --style=compressed fruitUI/src/scss/build:fruitUI/dist/css",
    "turbo:prod": "+ turbo run lint build && changeset version && changeset publish"
  },
  "workspaces": {
    "packages": ["packages/ui/package.json"],
    "docs": ["docs/package.json"]
  },
  "devDependencies": {
    "typescript": "latest",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.5.1"
  },
  "dependencies": {
    "@changesets/cli": "^2.26.2",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.0"
  }
}
